<h1>Listing task_completions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @task_completions.each do |task_completion| %>
      <tr>
        <td><%= link_to 'Show', task_completion %></td>
        <td><%= link_to 'Edit', edit_task_completion_path(task_completion) %></td>
        <td><%= link_to 'Destroy', task_completion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Task completion', new_task_completion_path %>

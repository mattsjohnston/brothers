<h1>Listing goal_completions</h1>

<table>
  <thead>
    <tr>
      <th>Goal</th>
      <th>User</th>
      <th>Program</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @goal_completions.each do |goal_completion| %>
      <tr>
        <td><%= goal_completion.goal %></td>
        <td><%= goal_completion.user %></td>
        <td><%= goal_completion.program %></td>
        <td><%= link_to 'Show', goal_completion %></td>
        <td><%= link_to 'Edit', edit_goal_completion_path(goal_completion) %></td>
        <td><%= link_to 'Destroy', goal_completion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Goal completion', new_goal_completion_path %>
